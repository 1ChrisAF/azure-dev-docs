{
    "$schema": "file:///c:/Users/robreed/.vscode/extensions/teamtoolbox.docindex-0.0.1/resources/config/docindex.schema.json",        
    "auditRules": [                                  
      {
        "operation": "File_FileType_Equals",
        "fileType": "Article",
        "title": "File type is 'Article'",
        "ruleGroup": "Tutorial",
        "ruleSet": "MVC",
        "shouldWarnUser": false,
        "dependentList": [
          {
            "operation": "MetadataField_Matches",
            "text": "^tutorial$",
            "metadataField": "ms.topic",
            "title": "Topic must be 'tutorial'",
            "dependentOperation": "AllBlocks",
            "quickFix": {
              "query": "^ms.topic:[^\r\n]+",
              "insert": "ms.topic: tutorial",
              "message": "Change ms.topic to 'tutorial'",
              "operation": "Replace"
            },
            "dependentList": [
              {
                "operation": "MetadataField_Matches",
                "title": "Title contains 'tutorial'",
                "text": "tutorial",
                "metadataField": "title"             
              },
              {
                "operation": "MetadataField_Matches",
                "title": "Description contains 'tutorial'",
                "text": "tutorial",
                "metadataField": "title"             
              },
              {
                "operation": "MetadataField_Matches",
                "title": "Title does not start with a number",
                "text": "^[^\\d]",
                "metadataField": "title"             
              },
              {
                "operation": "Has_MD_Artifact",
                "title": "H1 starts with 'Tutorial'",
                "artifact":  {
                  "type": "H1",
                  "attributes": {
                    "text": "^# Tutorial:"
                  }
                },                
                "quickFix": {
                  "operation": "Replace",
                  "query": "^#( )*((Quickstart):( )*)?",
                  "insert": "# Tutorial: ",
                  "message": "Insert/Replace start of H1 with 'Tutorial:'"
                }
              },
              {
                "operation": "Has_MD_Artifact",
                "title": "First H2 must be 'Prerequisites'",                
                "artifact": {
                  "type": "H2",
                  "attributes": {
                    "text": "^## Prerequisites"
                  }
                },
                "atIndex": 0             
              },
              {
                "operation": "Each_MD_Artifact_Matches",
                "title": "H3's must not be numbered",
                "artifact": {
                  "type": "H3",
                  "attributes": {
                    "text": "^###( )*[^\\d]"        
                  }
                }                
              },
              {
                "operation": "Each_MD_Artifact_Matches",
                "title": "H2's must not be numbered",
                "artifact": {
                  "type": "H2",
                  "attributes": {
                    "text": "^##( )*[^\\d]"
                  }
                }              
              },
              {
                "operation": "Has_MD_Artifact",
                "title": "Has H1",
                "artifact": {
                  "type":"H1"
                },
                "dependentList": [
                  {
                    "operation": "Has_MD_Artifact",
                    "title": "Introduction paragraph must contain checklist",
                    "artifact":{
                      "type": "Checklist"                  
                    },                    
                    "quickFix": {
                      "operation": "Replace",
                      "query": "(?:.)(?=\\Z)",
                      "insert": "\r\n\r\n> [!div class=\"checklist\"]\r\n> - Do something.\r\n> - Do something else.\r\n> - Do even more things.\r\n> - Do the last thing.",
                      "message": "Insert a checklist"
                    }        
                  }
                ]                    
              },
              {
                "operation": "Has_MD_Artifact",
                "title": "'Next steps' is last H2",
                "artifact":{
                  "type": "H2",
                  "attributes": {
                    "text": "^##( )*Next steps"
                  }
                },
                "atIndex": -1                
              },
              {
                "operation": "Has_MD_Artifact",
                "title": "Has 'Next steps'",
                "artifact": {
                  "type": "H2",
                  "attributes": {
                    "text": "^##( )*Next steps"
                  }
                },                
                "dependentList": [
                  {
                    "operation": "Condition",
                    "title": "'Next steps' has 1 links and uses Blue Box format",
                    "logicalOperator": "And",
                    "conditionList":[
                      {
                        "operation": "Has_n_MD_Artifacts",
                        "title": "'Next steps' has 1 link",
                        "artifact": {
                          "type": "Link"
                        },
                        "artifactMaxCount": 1,
                        "artifactMinCount": 1                                                    
                      },
                      {
                        "operation": "Has_MD_Artifact",
                        "title": "'Next steps' uses Blue Box format",
                        "artifact": {
                          "type": "BlueBox"
                        },
                        "quickFix": {
                          "operation": "Replace",
                          "query": "(?:.)(?=\\Z)",
                          "insert": "\r\n\r\n> [!div class=\"nextstepaction\"]\r\n> [Next Step](link.md)",
                          "message": "Insert Blue Box"
                        }                                                                      
                      }      
                    ]                                
                  }
                ]                                
              }
            ]
          }          
        ]                                                
      },
      {
