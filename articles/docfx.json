{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/docfx.json",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ],
        "src": ".",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/developer/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {
      "author": {
        "java/**/**.md": "bmitchell287",
        "go/**/**.md": "barbkess"        
      },
      "ms.author": {
        "java/**/**.md": "brendm",
        "go/**/**.md": "barbkess"        
      },
      "manager": {
        "java/**/**.md": "barbkess",
        "go/**/*.md": "barbkess"        
      },
      "ms.devlang": {
        "java/**/**.md": "java",
        "go/**/**.md": "go"
      },
      "ms.service": {
        "java/**/**.md": "azure-java",
        "go/**/**.md": "azure"
      },
      "ms.topic": {
        "**/includes/**/**.md": "include"
      }
    },
    "template": [],
    "dest": "azure-dev-docs",
    "markdownEngineName": "markdig"
  }
}
