{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ],
        "src": ".",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/developer/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/azure-dev-docs"
    },
    "fileMetadata": {
      "feedback_product_url" : {
        "articles/python/**/*.md": "https://github.com/Azure/azure-sdk-for-python/issues"
      },
      "product_feedback_url": {
        "articles/python/tutorial-deploy-app-service-on-linux*.md" : "https://github.com/Microsoft/vscode-azureappservice/issues",
        "articles/python/tutorial-deploy-containers*.md" : "https://github.com/Microsoft/vscode-azureappservice/issues",
        "articles/python/tutorial-vs-code-serverless-python*.md" : "https://github.com/Microsoft/vscode-azurefunctions/issues"
      },     
      "_op_documentIdPathDepotMapping": {
        "articles/python": {
          "depot_name": "MSDN.python-sdk",
          "folder_relative_path_in_docset": "docs-ref-conceptual"
        }
      },
      "author": {
        "articles/go/**/*.md": "barbkess",
        "articles/python/**/*.md": "kraigb"        
      },
      "manager": {
        "articles/go/**/*.md": "barbkess",
        "articles/python/**/*.md": "barbkess" 
      },
      "ms.author": {
        "articles/go/**/*.md": "barbkess",
        "articles/python/**/*.md": "kraigb"
      },      
      "ms.devlang": {
        "articles/go/**/*.md": "go",
        "articles/python/**/*.md": "python"
      },
      "ms.topic": {
        "articles/go/includes/**/*.md": "include",
        "articles/python/includes/**/*.md": "include"
      },
      "ms.service": {
        "articles/*.md": "azure",
        "articles/go/*.md": "azure"
      },
      "ms.prod": {
        "articles/python/**/*.md": "azure-python"        
      }
    },
    "template": [],
    "dest": {
      "articles": "azure-dev-docs",
      "articles/go": "azure-go",
      "articles/python": "azure-python"
    },
    "markdownEngineName": "markdig"
  }
}
